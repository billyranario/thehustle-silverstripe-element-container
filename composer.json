{
    "name": "thehustle/silverstripe-element-container",
    "description": "A SilverStripe module providing container and column blocks.",
    "type": "silverstripe-vendormodule",
    "require": {
        "php": "^7.1 || ^8.0",
        "silverstripe/cms": "^4 || ^5",
        "silverstripe/framework": "^4 || ^5",
        "dnadesign/silverstripe-elemental": "^4 || ^5",
        "dnadesign/silverstripe-elemental-list": "^2.0@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^5 || ^9"
    },
    "autoload": {
        "psr-4": {
            "TheHustle\\Layout\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "templates"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your@email.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
